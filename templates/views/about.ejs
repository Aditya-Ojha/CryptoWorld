    <script type="text/javascript">
        //subscribe-------
        function subscribe() {
                document.getElementById('subDone').method = "POST";
                document.getElementById('subDone').action = "subscribe";

            }

        // signin -------------login-------
        const el = document.getElementById('loged');
        const list = document.getElementById('navList');
        let items = list.childNodes;
        let i;
        for (i = 0, size = items.length - 1; i < size - 1; i++) {
        }
        if (el.className === "sign") {
            items[i].innerHTML = "<a href='signup'>Signin/Login</a>";
        } else {
            items[i].innerHTML = "👤<%=user%> <span class='logged' onclick='logout()' style='padding:12px'><a href='#' style='color:black' >Logout</a></span>"
            items[i].classList.add('updatedParent');
        }

        function logout() {
            items[i].innerHTML = "<a href='signup'>Signin/Login</a>";
            items[i].classList.remove('updatedParent');
        }


        function validation() {
            var fname = document.getElementById('fname').value;
            var lname = document.getElementById('lname').value;
            var email = document.getElementById('email').value;
            var phone = document.getElementById('phone').value;


            if (fname == "") {
                document.getElementById('userfname').innerHTML = " * Please fill your first name";
                // alert(" * Enter your name")
                return false;
            }
            if (fname.length < 3 || fname.length > 20) {
                document.getElementById('userfname').innerHTML = " * Name length should be between 3 to 20";
                // alert(" * Name length should be between 3 to 20")
                return false;
            }
            if (!isNaN(fname)) {
                document.getElementById('userfname').innerHTML = " * Name cannot be a number";
                // alert(" * Name cannot be a number");
                return false;
            }
            if (document.getElementById('userfname').innerHTML.length > 0) {
                document.getElementById('userfname').innerHTML = "";
            }

            if (lname == "") {
                document.getElementById('userlname').innerHTML = " * Please fill your last name";
                // alert("Enter your last name");
                return false;
            }
            if (lname.length < 3 || lname.length > 20) {
                document.getElementById('userlname').innerHTML = " * Name length should be between 3 to 20";
                // alert("Name length should be between 3 to 20")
                return false;
            }
            if (!isNaN(lname)) {
                document.getElementById('userlname').innerHTML = " * Name cannot be a number";
                // alert(" * Name cannot be a number");
                return false;
            }
            if (document.getElementById('userlname').innerHTML.length > 0) {
                document.getElementById('userlname').innerHTML = "";
            }




            if (phone == "") {
                document.getElementById('phNumber').innerHTML = " * Enter your contact number";
                // alert(" * Enter your contact number");
                return false;
            }
            if (isNaN(phone)) {
                document.getElementById('phNumber').innerHTML = " * Please enter valid number";
                // alert(" * Please enter valid number");
                return false;
            }
            if (phone.length != 10) {
                document.getElementById('phNumber').innerHTML = " * Please enter valid number";
                // alert(" * Please enter valid number");
                return false;
            }
            if (document.getElementById('phNumber').innerHTML.length > 0) {
                document.getElementById('phNumber').innerHTML = "";
            }

            if (email == "") {
                document.getElementById('emailId').innerHTML = " * Please fill your email id";
                // alert(" * Please fill your email id");
                return false;
            }
            if (email.indexOf('@') <= 0) {
                document.getElementById('emailId').innerHTML = " * Enter valid email id";
                // alert(" * Enter valid email id");
                return false;
            }
            if ((email.charAt(email.length - 4) != '.') && (email.charAt(email.length - 3) != '.')) {
                document.getElementById('emailId').innerHTML = " * Please enter valid domain like .com or .in";
                // alert(" * Please enter valid domain (eg: '.com' or '.in'");
                return false;
            }
            if (document.getElementById('emailId').innerHTML.length > 0) {
                document.getElementById('emailId').innerHTML = "";
            }

            //If all cases pass then it will submit result in database with method POST and action /add
            alert("Form Submitted Successfully☻");

            document.getElementById('formSubmission').method = "POST";
            document.getElementById('formSubmission').action = "/add";

        }


